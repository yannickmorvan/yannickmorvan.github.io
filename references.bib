@techreport{knechtReshapingAcademicSelf2021,
  title = {Reshaping the {{Academic Self}}: {{Connecting Education}} \& {{Open Science}}},
  shorttitle = {Reshaping the {{Academic Self}}},
  author = {de Knecht, Sicco and van der Meer, Martijn and Brinkman, Loek and Kluijtmans, Manon and Miedema, Frank},
  year = {2021},
  month = aug,
  institution = {{Zenodo}},
  doi = {10.5281/zenodo.5345573},
  urldate = {2021-09-06},
  abstract = {The transition towards Open Science will drastically alter our approach to academic life. It will change the ways in which we reward and recognise university employees and reshape the relationship between education and research. This should be reflected in how a new generation of academics and citizens are educated. Not only through the qualifications our students receive to become productive members of society, but also by dint of the values and attitudes we teach our pupils. The aim of university education should be preparing future graduates to share their (inter)disciplinary knowledge, engage with societal stakeholders, and shape tomorrow's society. Now is the time to explore how. This manifesto is a thought exercise that explores the (possible) relationships between Open Science and education. It attempts to point out the overlap, parallels, synergy, and possible conflicts between Open Science attitudes and practices, and contemporary views and practices in education. We aim to provoke a perspective~on the different aspects of how Open Science relates to education and propose several concrete directions forward and possible corresponding interventions. After explaining why education from an Open Science perspective needs to be explored and strengthened, we differentiate four faces of open education: the Open Science mindset, Open Science skillset, open educational resources, and how these activities should be recognised and rewarded. We subsequently illuminate three possible paths on how to strengthen open education, ranging from content to form and system. We hope that this will spark a broader national and international conversation on the relationship between Open Science and education.},
  langid = {english},
  keywords = {Education,Open Educational Resources,Open Science,Recognition and Rewards,Reflexivity},
  file = {C\:\\Users\\yanni\\OneDrive - Université paris nanterre\\Documents\\Zotero\\storage\\ZCTVZBEH\\Knecht et al. - 2021 - Reshaping the Academic Self Connecting Education .pdf}
}


