@inproceedings{morvanScienceOuvertePardela2023,
  title = {{Science Ouverte, par-del\`a la partie \'emerg\'ee de l'Acc\`es Ouvert}},
  booktitle = {{Open Access Week 2023}},
  author = {Morvan, Yannick},
  year = {2023},
  month = oct,
  address = {{Nanterre}},
  abstract = {Le terme de science ouverte rev\^et un caract\`ere \guillemotleft slogan\guillemotright{} et agr\`ege diff\'erents aspects. Si l'accessibilit\'e aux productions de la science (comme les r\'epertoires publics de d\'ep\^ot des publications tels que HAL ou de d\'ep\^ot de pre-print/pr\'e-publication comme PsyArXiv) peut occuper une place importante de la science ouverte, elle ne saurait la r\'esumer. En effet, on observe parfois une confusion voire une r\'eduction des pratiques de science ouverte \`a la simple mise \`a disposition sans barri\`ere d'acc\`es des manuscrits de recherche dans HAL ou PsyArXiv. Il est important de consid\'erer cinq autres \guillemotleft piliers\guillemotright{} (en plus de l'acc\`es aux publications) qui soutiennent la science ouverte. Il s'agit : 1) des donn\'ees ouvertes et des r\'esultats reproductibles ; 2) de l'ouverture des \'evaluations, des opinions et des critiques (exemple de l'Open Peer Review ou \guillemotleft\'evaluation ouverte par les pairs\guillemotright{} et des plateformes de post-publications comme PubPeer) ; 3) des politiques incitatives et de reconnaissances pour favoriser le changement (comme les initiatives de type DORA et de leurs cons\'equences sur les institutions et organismes de recherche) ; 4) des technologies qui facilitent l'adoption des pratiques de science ouverte (\`a l'aide d'outils tels que OSF et GitHub) ; 5) d'une plus grande implication de la soci\'et\'e dans la science et ses productions (\`a l'image du mouvement dit de la science participative ou du label \guillemotleft Science avec et pour la Soci\'et\'e\guillemotright ). Cette diversit\'e des \guillemotleft piliers\guillemotright{} qui soutiennent la science ouverte est justement ce qui permet \`a diff\'erents acteurs (des chercheurs aux professionnels de terrain jusqu'aux citoyens) d'investir ce champ avec des comp\'etences diverses et compl\'ementaires.},
  langid = {french}
}


