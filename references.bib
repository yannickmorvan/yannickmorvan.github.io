@article{borsboomSmallWorldPsychopathology2011,
  title = {The {{Small World}} of {{Psychopathology}}},
  author = {Borsboom, Denny and Cramer, Ang{\'e}lique O. J. and Schmittmann, Verena D. and Epskamp, Sacha and Waldorp, Lourens J.},
  year = {2011},
